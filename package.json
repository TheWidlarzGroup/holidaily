{
  "name": "Holidaily",
  "version": "1.0.39",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "detox:android": "detox test -c android.emu.debug",
    "detox:android-build": "detox build -c android.emu.debug",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --config .prettierrc --write src/**/*.{js,jsx,ts,tsx}",
    "type-check": "tsc --noEmit",
    "ios:beta": "fastlane ios beta",
    "android:beta": "fastlane android beta"
  },
